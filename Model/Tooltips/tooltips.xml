<?xml version="1.0"?>
<PropertyList>
  <model>
      <name>main-panel</name>
      <path>Aircraft/tu154b/Model/Tooltips/main-panel.xml</path>
  </model>
  <model>
      <name>center-panel</name>
      <path>Aircraft/tu154b/Model/Tooltips/center-panel.xml</path>
  </model>
  <model>
      <name>pedestal-panel</name>
      <path>Aircraft/tu154b/Model/Tooltips/pedestal-panel.xml</path>
      <offsets>
        <x-m>-22.51</x-m>
        <y-m>0</y-m>
        <z-m>1.4</z-m>
        <pitch-deg>-90</pitch-deg>
      </offsets>
  </model>
  <model>
    <name>overhead-panel</name>
    <path>Aircraft/tu154b/Model/Tooltips/overhead-panel.xml</path>
    <offsets>
      <x-m>-22.172</x-m>
      <y-m>0.0</y-m>
      <z-m>2.4</z-m>
      <pitch-deg>69.0</pitch-deg>
    </offsets>
</model>
  <model>
      <name>engineer-panel</name>
      <path>Aircraft/tu154b/Model/Tooltips/engineer-panel.xml</path>
      <offsets>
        <x-m>-21.1</x-m>
        <y-m>0.91</y-m>
        <z-m>2.26</z-m>
        <pitch-deg>0</pitch-deg>
        <heading-deg>-90</heading-deg>
      </offsets>
  </model>
  <animation>
    <type>material</type>
    <object-name>main-panel</object-name>
    <object-name>overhead-panel</object-name>
    <object-name>center-panel</object-name>
    <object-name>pedestal-panel</object-name>
    <object-name>engineer-panel</object-name>
    <diffuse>
      <red>0.0</red>
      <green>0.0</green>
      <blue>0.0</blue>
    </diffuse>
    
    <emission>
      <red-prop>tu154/options/tooltips-color/r</red-prop>
      <green-prop>tu154/options/tooltips-color/g</green-prop>
      <blue-prop>tu154/options/tooltips-color/b</blue-prop>
    </emission>
</animation>
  <animation>
    <type>select</type>
    <object-name>main-panel</object-name>
    <object-name>overhead-panel</object-name>
    <object-name>center-panel</object-name>
    <object-name>pedestal-panel</object-name>
    <object-name>engineer-panel</object-name>
    <condition>
        <property>/sim/panel-hotspots</property>
    </condition>
  </animation>
  
</PropertyList>
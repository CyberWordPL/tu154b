  <!-- Airports -->
  <animation>
    <type>pick</type>
    <object-name>ndap_h</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <binding>
        <command>property-toggle</command>
        <property>instrumentation/efis/inputs/arpt</property>

  <!-- Cabin Windows -->
  <animation>
    <type>pick</type>
    <object-name>LeftWindowHandle</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>
          var pos = getprop("tu154/door/window-left-sec");
          if (pos) {
              interpolate("tu154/door/window-left-sec", 0, pos * 2);
          } else {
              interpolate("tu154/door/window-left-sec", 1, 2);
          }
        </script>
      </binding>
    </action>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>LeftWindow</object-name>
    <object-name>LeftWindowHandle</object-name>
    <object-name>LeftWindowGlass</object-name>
    <property>tu154/door/window-left</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>0.1</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>0.25</ind>
        <dep>-5</dep>
      </entry>
      <entry>
        <ind>1</ind>
        <dep>-3</dep>
      </entry>
    </interpolation>
    <axis>
      <x1-m>0.7706</x1-m>
      <y1-m>-0.70364</y1-m>
      <z1-m>1.276</z1-m>
      <x2-m>0.4761</x2-m>
      <y2-m>-0.82664</y2-m>
      <z2-m>0.94192</z2-m>
    </axis>
  </animation>
  <animation>

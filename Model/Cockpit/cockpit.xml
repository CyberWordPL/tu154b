    </action>
  </animation>
  <!-- Center -->
  <animation>
    <type>pick</type>
    <object-name>ndctr_h</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <binding>
        <command>property-toggle</command>
        <property>instrumentation/efis/inputs/nd-centered</property>
      </binding>
    </action>
  </animation>
  <!-- Range -->
  <animation>
    <type>pick</type>
    <object-name>ndrange_h</object-name>
    <condition>
      <less-than>
        <property>instrumentation/efis/inputs/range-nm</property>
        <value>640</value>
      </less-than>
    </condition>
    <action>
      <button>3</button>
      <!--  scroll down -->
      <repeatable>false</repeatable>   
      <binding>
        <command>property-cycle</command>
        <property>instrumentation/efis/inputs/range-nm</property>
        <value>10</value>
        <value>20</value>
        <value>40</value>
        <value>80</value>
        <value>160</value>
        <value>320</value>
        <value>640</value>
      </binding>   
    </action>
  </animation>
  <animation>
    <type>pick</type>
    <object-name>ndrange_h</object-name>
    <condition>
      <greater-than>
        <property>instrumentation/efis/inputs/range-nm</property>
        <value>10</value>
      </greater-than>
    </condition>
    <action>
      <button>4</button>
      <!--  scroll up -->
      <repeatable>false</repeatable>
      <binding>
        <command>property-cycle</command>
        <property>instrumentation/efis/inputs/range-nm</property>
        <value>640</value>
        <value>320</value>
        <value>160</value>
        <value>80</value>
        <value>40</value>
        <value>20</value>
        <value>10</value>              
      </binding>   
    </action>
  </animation>

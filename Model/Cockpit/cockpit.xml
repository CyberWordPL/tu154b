  <!-- Mapcase -->
  <animation>
    <type>material</type>
    <object-name>mapcase</object-name>
    <emission>
      <red-prop>sim/model/cabin-lighting/prib</red-prop>
      <green-prop>sim/model/cabin-lighting/prib</green-prop>
      <blue-prop>sim/model/cabin-lighting/prib-2</blue-prop>
      <factor>0.5</factor>
    </emission>
  </animation>
  <animation>
    <type>material</type>
    <object-name>ndplace</object-name>
    <emission>
      <red-prop>sim/model/cabin-lighting/lamps</red-prop>
      <green-prop>sim/model/cabin-lighting/lamps</green-prop>
      <blue-prop>sim/model/cabin-lighting/lamps</blue-prop>
      <factor>1.5</factor>
    </emission>
  </animation>
  <animation>
    <type>material</type>
    <object-name>ndrange</object-name>
    <object-name>ndwx</object-name>
    <object-name>ndterr</object-name>
    <object-name>ndtfc</object-name>
    <object-name>ndctr</object-name>
    <object-name>ndap</object-name>
    <emission>
      <red-prop>sim/model/cabin-lighting/lamps</red-prop>
      <green-prop>sim/model/cabin-lighting/lamps</green-prop>
      <blue-prop>sim/model/cabin-lighting/lamps</blue-prop>
    </emission>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>mapcase_hs</object-name>
    <visible>false</visible>
    <increase>
      <binding>
        <command>nasal</command>
        <script>mapcase.switch_page(1)</script>
      </binding>
    </increase>
    <decrease>
      <binding>
        <command>nasal</command>
        <script>mapcase.switch_page(-1)</script>
      </binding>
    </decrease>
    <hovered>
      <binding>
        <command>set-tooltip</command>
        <tooltip-id>mapcase-page</tooltip-id>
        <label>Page %s</label>
        <property>tu154/instrumentation/mapcase/page</property>
        <mapping>nasal</mapping>
        <script>
          var page = arg[0];
          var pages = getprop("tu154/instrumentation/mapcase/total_pages");
          return sprintf("%d/%d", page, pages);
        </script>
      </binding>
    </hovered>
  </animation>

  <!--************************* Nav Display ***************************-->
  <animation>
    <type>blend</type>
    <object-name>ndplace</object-name>
    <object-name>ndrange</object-name>
    <object-name>ndwx</object-name>
    <object-name>ndterr</object-name>
    <object-name>ndtfc</object-name>
    <object-name>ndctr</object-name>
    <object-name>ndap</object-name>
    <property>instrumentation/ND/show</property>
    <factor>-1</factor>
    <offset>1</offset>
  </animation>

<?xml version="1.0"?>

<PropertyList>

  <title>Tu-154B-2</title>
  <key>
    <name>U</name>
    <desc>Autostart (aUtostart)</desc>
  </key>
  <key>
    <name>Ctrl+C</name>
    <desc>Floating tooltips (additional descriptions) and mouse zones</desc>
  </key>
  <key>
    <name>F/Shift+F/Ctrl+F</name>
    <desc>AP On/Autothrottle On/AP-AT Off (F - autoFlight)</desc>
  </key>
  <key>
    <name>Shift+N/N/Ctrl+N/*</name>
    <desc>Set nominal/0.85/0.7/0.0 thrust</desc>
  </key>
  <key>
    <name>J/K</name>
    <desc>Spoilers control</desc>
  </key>
  <key>
    <name>R</name>
    <desc>Thrust reverser On (ReveRseR)</desc>
  </key>
  <key>
    <name>M</name>
    <desc>Center aileron, elevator, and rudder (in Middle)</desc>
  </key>
  <key>
    <name>G/Shift+G/Ctrl+G</name>
    <desc>Gear up, down, neutral</desc>
  </key>
  <key>
    <name>[Keyboard Mode: Alt+] 1-9</name>
    <desc>Fast Views Switches</desc>
  </key>
  <key>
    <name>`</name>
    <desc>Closer view for views 1-5 (until released)</desc>
  </key>
  <key>
    <name>[Disabled in Keyboard Mode] Alt+1-3,5-7</name>
    <desc>Alternative views for views 1-3, 5-7</desc>
  </key>
  <key>
    <name>C</name>
    <desc>Reset current View</desc>
  </key>

</PropertyList>

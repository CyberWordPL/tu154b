<!-- "Busters" indicators on FE panel
3D instrument for Tu-154 -->
<PropertyList>
 <path>busters.ac</path>
<!--External panel lighting-->
<animation>
  <type>material</type>
  <object-name>cover</object-name>
  <emission>
    <red-prop>tu154/light/panel/ext-red</red-prop>
    <green-prop>tu154/light/panel/ext-green</green-prop>
    <blue-prop>tu154/light/panel/ext-blue</blue-prop>
  </emission>
</animation>
<!--Indicators-->
<animation>
  <type>material</type>
  <object-name>text</object-name>
  <emission>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
    <factor-prop>tu154/switches/busters-cover</factor-prop>
  </emission>
</animation>



</PropertyList>

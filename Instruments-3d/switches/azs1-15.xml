<?xml version="1.0"?>
<PropertyList>
  <params>
    <switch/>
  </params>
  <path>azs_1.ac</path>

  <!--External panel lighting-->
  <animation>
    <type>material</type>
    <object-name>handle</object-name>
    <emission>
      <red-prop>sim/model/cabin-lighting/vnesh</red-prop>
      <green-prop>sim/model/cabin-lighting/vnesh</green-prop>
      <blue-prop>sim/model/cabin-lighting/vnesh</blue-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>handle</object-name>
    <property alias="../../params/switch"/>
    <factor>120</factor>
    <offset-deg>-60</offset-deg>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>

  <!--Control-->
  <animation>
    <type>pick</type>
    <object-name>hotspot_1</object-name>
    <visible>false</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-interpolate</command>
        <property alias="../../../../params/switch"/>
        <value>1</value>
        <time>0.1</time>
      </binding>
    </action>
  </animation>
  <animation>
    <type>pick</type>
    <object-name>hotspot_2</object-name>
    <visible>false</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-interpolate</command>
        <property alias="../../../../params/switch"/>
        <value>0</value>
        <time>0.1</time>
      </binding>
    </action>
  </animation>

</PropertyList>

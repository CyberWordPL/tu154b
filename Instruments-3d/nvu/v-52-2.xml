<!-- V-52 NVU (navigation system) control
3D instrument for Tu-154 -->
<PropertyList>
 <path>v-52.ac</path>
<!--External panel lighting-->
<animation>
  <type>material</type>
  <object-name>base</object-name>
    <emission>
    <red-prop>tu154/light/panel/ext-red</red-prop>
    <green-prop>tu154/light/panel/ext-green</green-prop>
    <blue-prop>tu154/light/panel/ext-blue</blue-prop>
  </emission>
</animation>

<!-- Internal instruments lighting -->
<animation>
  <type>material</type>
  <object-name>text</object-name>
  <object-name>digits</object-name>
  <emission>
    <red-prop>tu154/light/instruments/int-red</red-prop>
    <green-prop>tu154/light/instruments/int-green</green-prop>
    <blue-prop>tu154/light/instruments/int-blue</blue-prop>
  </emission>
</animation>
<!--Indicators-->
<animation>
  <type>material</type>
  <object-name>aircraft</object-name>
  <object-name>point</object-name>
  <object-name>beacon</object-name>
  <emission>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
    <factor-prop>tu154/systems/electrical/checking-lamps/lamps-power</factor-prop>
  </emission>
</animation>
<animation>
  <type>select</type>
  <object-name>aircraft</object-name>
  <condition>
    <property>tu154/instrumentation/v-52[1]/ind-aircraft</property>
  </condition>
</animation>
<animation>
  <type>select</type>
  <object-name>point</object-name>
  <condition>
    <property>tu154/instrumentation/v-52[1]/ind-point</property>
  </condition>
</animation>
<animation>
  <type>select</type>
  <object-name>beacon</object-name>
  <condition>
    <property>tu154/instrumentation/v-52[1]/ind-beacon</property>
  </condition>
</animation>
<!--Blankers for digits-->
<animation>
  <type>select</type>
  <object-name>z_ones_1</object-name>
  <object-name>z_dec_1</object-name>
